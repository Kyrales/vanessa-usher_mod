{
  "globals": {
    "storage-user": "admin",
    "storage-pwd": "admin",
    "lic-try-count": 5,
    "plugins": {
      "enable": [
        "increment",
        "check-authors",
        "sync-remote"
      ]
    },
    "plugins-config": {
      "rename-module": true,
      "rename-form": true,
      "pull": true,
      "push": true,
      "push-n-commits": 0,
      "push-tags": true,
      "skip-exists-tags": false
    }
  },
  "repositories": [
    {
      "name": "my-base",
      "path": "http://storage-server1C:1234/my-base",
      "dir": "./src/cf",
      "v8version": "8.3.15.1489",
      "plugins-config": {
        "URL": "https://github.com/MyCompany/my-project.git"
      }
    }
  ]
}